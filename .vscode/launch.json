{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}",
            "env": {
                "USERNAME": "user",
                "PASSWORD": "passwd",
                "WEBHOOK": "http://127.0.0.1:8080/notify?title={{.title}}&content={{.content}}"
              }
        }
    ]
}